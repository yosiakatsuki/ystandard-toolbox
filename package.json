{
  "name": "ystandard-toolbox",
  "version": "1.0.0",
  "description": "無料WordPressテーマ「yStandard」用機能拡張プラグイン",
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@codemirror/lang-css": "^6.0.0",
    "@codemirror/lang-html": "^6.0.0",
    "@codemirror/language": "^6.2.0",
    "@emotion/babel-plugin": "^11.9.2",
    "@uiw/react-codemirror": "^4.8.1",
    "@wordpress/block-serialization-default-parser": "^4.11.0",
    "@wordpress/blocks": "^11.10.0",
    "@wordpress/browserslist-config": "^4.1.2",
    "@wordpress/env": "^4.9.0",
    "@wordpress/icons": "^9.2.0",
    "@wordpress/prettier-config": "^1.4.0",
    "@wordpress/scripts": "^23.3.0",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-inline-json-import": "^0.3.2",
    "browserslist": "^4.20.3",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "css-declaration-sorter": "^6.2.2",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.11",
    "del": "^6.1.1",
    "es6-promise": "^4.2.8",
    "fast-glob": "^3.2.11",
    "feather-icons": "^4.29.0",
    "hex-rgb": "^5.0.0",
    "husky": "^8.0.1",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "po2json": "^1.0.0-beta-3",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "prettier": "npm:wp-prettier@^2.0.5",
    "prop-types": "^15.7.2",
    "query-string": "^7.1.1",
    "react-feather": "^2.0.10",
    "react-toastify": "^9.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "simple-icons": "^7.0.0",
    "style-loader": "^3.3.1",
    "swiper": "^8.2.2",
    "vue": "^2.6.14",
    "vue-color": "^2.8.1",
    "vue-feather-icons": "^5.1.0",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.14",
    "vue-toasted": "^1.1.28",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2",
    "webpack": "^5.73.0"
  },
  "scripts": {
    "open": "open http://localhost:10020/",
    "activate:theme": "wp-env run cli 'wp theme activate ystandard'",
    "start:update": "wp-env start --xdebug -- --update && npm run activate:theme && npm run import",
    "start": "wp-env start --xdebug && npm run import && npm run open",
    "stop": "npm run export && wp-env stop",
    "lint:js:src": "wp-scripts lint-js src/js/**/*.js ./src/js --fix",
    "lint:js:blocks": "wp-scripts lint-js blocks ./src/js --fix",
    "lint:js": "run-p lint:js:*",
    "lint:css:src": "wp-scripts lint-style 'src/**/*.scss' --fix",
    "lint:css:blocks": "wp-scripts lint-style 'blocks/**/*.scss' --fix",
    "lint:css": "run-s lint:css:*",
    "lint:php": "composer phpcs",
    "lint": "run-s lint:*",
    "watch:js:block": "wp-scripts start --config webpack.blocks.config.js",
    "watch:js:block-app": "wp-scripts start --config webpack.block-app.config.js",
    "watch:js:app": "babel src/js/app --out-dir js/app -w -s",
    "watch:js:admin": "webpack build --config ./webpack.menu.config.dev.js -w --mode development",
    "watch:js:plugin-settings": "wp-scripts start --config webpack.plugin-settings.config.js",
    "watch:js": "run-p watch:js:*",
    "watch:css:postcss": "postcss temp/css/**/*.css --base temp/css --dir css --verbose -w",
    "watch:css:sass": "sass src/sass:temp/css ./blocks:temp/css/blocks src/js:temp/css --no-source-map --no-stop-on-error -w",
    "watch:css": "run-p watch:css:*",
    "watch:clean:cache": "rm -rf ./node_modules/.cache",
    "watch": "NODE_ENV=development && npm run watch:clean:cache && run-p watch:*",
    "build:block": "wp-scripts build --config webpack.blocks.config.js",
    "build:block-app": "wp-scripts build --config webpack.block-app.config.js",
    "build:app": "babel src/js/app --out-dir js/app",
    "build:admin": "webpack build --config ./webpack.menu.config.js",
    "build:icons": "node src/js/script/create-svg-icon-list.js",
    "build": "NODE_ENV=production run-s build:*",
    "test:unit:php": "wp-env start && wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/ystandard-toolbox/phpunit.xml.dist --verbose'",
    "test:unit:js": "wp-scripts test-unit-js --config test/unit/jest.config.js",
    "test:unit": "run-s test:unit:*",
    "test": "run-s test:*",
    "format": "wp-scripts format",
    "fixtures:clean": "rimraf \"test/integration/fixtures/blocks/*.+(json|serialized.html)\"",
    "fixtures:generate": "cross-env GENERATE_MISSING_FIXTURES=y npm run test:unit:js test/integration/full-content/ && npm run format test/integration/fixtures/blocks/*.json",
    "fixtures:regenerate": "npm run fixtures:clean && npm run fixtures:generate",
    "fixtures": "npm run fixtures:regenerate test/integration/full-content/full-content.test.js",
    "i18n:make-pot": "wp i18n make-pot . languages/ystandard-toolbox.pot --domain=\"ystandard-toolbox\" --exclude=\".github,assets,bin,css,js,languages,library,tests\"",
    "i18n:blocks": "po2json languages/ystandard-toolbox-ja.po languages/ystandard-toolbox-ja-ystandard-toolbox-block-editor.json -f jed",
    "zip:copy:base": "rsync -a --exclude='.*' --exclude='/node_modules' --exclude='/vendor' --exclude='/bin' --exclude='/upload' --exclude='/composer.*' --exclude='*.config.js' --exclude='*.config.json' --exclude='*.xml.dist' --exclude='/phpunit' --exclude='/test' --exclude='/src' --exclude='package*.json' --exclude='/blocks' --exclude='/temp' --exclude='ystandard-toolbox*.json' . ystandard-toolbox",
    "zip:copy:blocks": "rsync -am --exclude='*.scss' --exclude='*.js' --exclude='test/' --exclude='deprecated/' ./blocks ystandard-toolbox/",
    "zip:archive": "zip -9 -qmr ystandard-toolbox.zip ystandard-toolbox",
    "zip": "run-s clean:upload clean:zip zip:copy:base zip:copy:blocks zip:archive",
    "mkdir:upload": "mkdir upload",
    "clean:temp": "rimraf temp",
    "clean:upload": "rimraf upload & rimraf ystandard-toolbox",
    "clean:css": "rimraf css",
    "clean:js": "rimraf js",
    "clean:zip": "rimraf ystandard-toolbox.zip",
    "copy:json:production": "cp ystandard-toolbox.json upload/ystandard-toolbox.json",
    "copy:json:develop": "cp ystandard-toolbox-beta.json upload/ystandard-toolbox.json",
    "copy:plugin": "cp ystandard-toolbox.zip upload/ystandard-toolbox.zip",
    "copy:production": "run-s mkdir:upload copy:json:production copy:plugin",
    "copy:develop": "run-s mkdir:upload copy:json:develop copy:plugin",
    "packages-update": "wp-scripts packages-update",
    "update": "run-p update:*",
    "cli": "wp-env run cli",
    "wp": "wp-env run cli wp",
    "export:uploads": "npm run cli 'cp -r wp-content/uploads .wp-content'",
    "export:db": "npm run wp 'db export .wp-content/db.sql'",
    "export:clean:remove": "npm run cli 'rm -rf .wp-content/uploads'",
    "export:clean:mkdir": "npm run cli 'mkdir -p .wp-content/uploads'",
    "export:clean": "run-s export:clean:remove export:clean:mkdir",
    "export": "npm run export:clean && run-p export:uploads export:db",
    "import:uploads": "npm run cli 'cp -r .wp-content/uploads wp-content'",
    "import:db": "npm run wp 'db import .wp-content/db.sql'",
    "import": "run-s import:*",
    "theme:clean": "rimraf .wp-content/themes",
    "theme:mkdir": "mkdir -p .wp-content/themes",
    "theme:copy": "rsync -a --exclude='.*' --exclude='node_modules' --exclude='vendor' ../ystandard .wp-content/themes",
    "theme:import": "npm run cli 'cp -rf .wp-content/themes/ystandard wp-content/themes'",
    "theme": "run-s theme:clean theme:mkdir theme:copy theme:import theme:clean",
    "prepare": "husky install"
  },
  "author": "yosiakatsuki",
  "license": "GPL-2.0+",
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
