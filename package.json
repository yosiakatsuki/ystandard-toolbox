{
  "name": "ystandard-toolbox",
  "version": "1.0.0",
  "description": "無料WordPressテーマ「yStandard」用機能拡張プラグイン",
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@wordpress/browserslist-config": "^4.1.0",
    "@wordpress/element": "^4.0.0",
    "@wordpress/icons": "^6.1.0",
    "@wordpress/scripts": "^17.1.0",
    "autoprefixer": "^10.3.1",
    "axios": "^0.21.2",
    "babel-loader": "^8.2.2",
    "classnames": "^2.3.1",
    "css-declaration-sorter": "^6.1.1",
    "css-loader": "^5.1.3",
    "cssnano": "^5.0.7",
    "del": "^6.0.0",
    "es6-promise": "^4.2.8",
    "feather-icons": "^4.28.0",
    "fibers": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^5.1.0",
    "hex-rgb": "^5.0.0",
    "husky": "^7.0.1",
    "lodash": "^4.17.21",
    "po2json": "^1.0.0-beta-3",
    "prettier": "npm:wp-prettier@^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-transition-group": "^4.4.2",
    "sass": "^1.36.0",
    "sass-loader": "^10.1.1",
    "simple-icons": "^5.8.0",
    "style-loader": "^2.0.0",
    "swiper": "^7.2.0",
    "vue": "^2.6.14",
    "vue-color": "^2.8.1",
    "vue-feather-icons": "^5.1.0",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.14",
    "vue-toasted": "^1.1.28",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2",
    "webpack": "^4.46.0",
    "webpack-stream": "^6.1.2"
  },
  "scripts": {
    "lint-js": "wp-scripts lint-js blocks ./src/js",
    "lint-js:fix": "npm run lint-js -- --fix",
    "lint-php": "composer phpcs",
    "lint": "npm run lint-js:fix && npm run lint-php",
    "watch:block": "wp-scripts start --config webpack.blocks.config.dev.js",
    "watch": "gulp watch & npm run watch:block",
    "build:block": "wp-scripts build --config webpack.blocks.config.js",
    "build:icons": "node src/js/script/create-svg-icon-list.js",
    "build": "gulp build && npm run build:block",
    "build:deploy-files": "gulp deployFiles",
    "build:deploy-files-beta": "gulp deployFilesBeta",
    "i18n:make-pot": "wp i18n make-pot . languages/ystandard-toolbox.pot --domain=\"ystandard-toolbox\" --exclude=\".github,assets,bin,css,js,languages,library,tests\"",
    "i18n:blocks": "po2json languages/ystandard-toolbox-ja.po languages/ystandard-toolbox-ja-ystandard-toolbox-block-editor.json -f jed",
    "packages-update": "wp-scripts packages-update",
    "production": "npm run build && npm run build:deploy-files",
    "production:manual": "npm run build && gulp manualProduction"
  },
  "author": "yosiakatsuki",
  "license": "GPL-2.0+",
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
