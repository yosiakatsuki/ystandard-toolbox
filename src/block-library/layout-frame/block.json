{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ystdtb/layout-frame",
  "version": "0.0.1",
  "title": "Layout Frame",
  "category": "ystdtb-layout",
  "description": "ブロックの大きさ・余白の設定ができるレイアウト用ブロック",
  "keywords": [
    "layout",
    "frame",
    "フレーム"
  ],
  "attributes": {
    "layout": {
      "type": "object"
    },
    "gap": {
      "type": "object"
    },
    "width": {
      "type": "object"
    },
    "height": {
      "type": "object"
    },
    "ratio": {
      "type": "object"
    },
    "margin": {
      "type": "object"
    },
    "padding": {
      "type": "object"
    },
    "zIndex": {
      "type": "string"
    }
  },
  "supports": {
    "html": false,
    "className": false,
    "align": [
      "full"
    ]
  },
  "textdomain": "ystandard-toolbox",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
