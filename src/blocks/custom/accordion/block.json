{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ystdtb/accordion",
  "version": "0.0.1",
  "title": "アコーディオン",
  "description": "開閉式ブロック",
  "category": "ystdtb_beta",
  "keywords": [
    "アコーディオン",
    "accordion"
  ],
  "attributes": {
    "isDefaultOpen": {
      "type": "boolean",
      "default": false
    },
    "summaryText": {
      "type": "string",
      "source": "html",
      "selector": ".ystdtb-accordion__summary-text"
    }
  },
  "supports": {
    "anchor": false,
    "align": false,
    "className": false
  },
  "textdomain": "ystandard-toolbox",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
