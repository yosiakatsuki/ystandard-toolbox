{
	"custom": {
		"label": "カスタム",
		"style": {},
		"before": {},
		"after": {}
	},
	"border-bottom": {
		"label": "下線",
		"style": {
			"border": {
				"bottom": {
					"color": "#222222",
					"style": "solid",
					"width": "1px"
				}
			},
			"padding": {
				"bottom": "0.5em"
			}
		}
	},
	"border-bottom-dotted": {
		"label": "下線(点線)",
		"style": {
			"border": {
				"bottom": {
					"color": "#222222",
					"style": "dotted",
					"width": "1px"
				}
			},
			"padding": {
				"bottom": "0.5em"
			}
		}
	},
	"border-bottom-dashed": {
		"label": "下線(破線)",
		"style": {
			"border": {
				"bottom": {
					"color": "#222222",
					"style": "dashed",
					"width": "1px"
				}
			},
			"padding": {
				"bottom": "0.5em"
			}
		}
	},
	"border-left": {
		"label": "左線",
		"style": {
			"border": {
				"left": {
					"color": "#222222",
					"style": "solid",
					"width": "4px"
				}
			},
			"padding": {
				"top": "0.3em",
				"bottom": "0.3em",
				"left": "0.5em"
			}
		}
	},
	"border-horizontal": {
		"label": "上下の直線",
		"style": {
			"border": {
				"top": {
					"color": "#222222",
					"style": "solid",
					"width": "1px"
				},
				"bottom": {
					"color": "#222222",
					"style": "solid",
					"width": "1px"
				}
			},
			"padding": {
				"top": "0.5em",
				"bottom": "0.5em"
			}
		}
	},
	"background-color": {
		"label": "背景色",
		"style": {
			"backgroundColor": "#eeeeee",
			"padding": {
				"top": "0.5em",
				"right": "0.5em",
				"bottom": "0.5em",
				"left": "0.5em"
			}
		}
	},
	"border-left-plus-bg": {
		"label": "左線 + 背景色",
		"style": {
			"backgroundColor": "#f7f7f7",
			"border": {
				"left": {
					"color": "#222222",
					"style": "solid",
					"width": "4px"
				}
			},
			"padding": {
				"top": "0.5em",
				"right": "0.5em",
				"bottom": "0.5em",
				"left": "0.5em"
			}
		}
	},
	"stitch": {
		"label": "ステッチ",
		"style": {
			"backgroundColor": "#eeeeee",
			"border": {
				"top": {
					"color": "#ffffff",
					"style": "dashed",
					"width": "2px"
				},
				"right": {
					"color": "#ffffff",
					"style": "dashed",
					"width": "2px"
				},
				"bottom": {
					"color": "#ffffff",
					"style": "dashed",
					"width": "2px"
				},
				"left": {
					"color": "#ffffff",
					"style": "dashed",
					"width": "2px"
				}
			},
			"padding": {
				"top": "0.5em",
				"right": "0.5em",
				"bottom": "0.5em",
				"left": "0.5em"
			},
			"boxShadow": "0 0 0 8px var(--ystdtb-custom-header-bg-color)",
			"borderRadius": "2px"
		}
	},
	"double-border-bottom": {
		"label": "下線2種類",
		"style": {
			"border": {
				"bottom": {
					"color": "#eeeeee",
					"style": "solid",
					"width": "2px"
				}
			},
			"padding": {
				"bottom": "0.5em"
			},
			"position": "relative"
		},
		"after": {
			"content": "\"\"",
			"height": "2px",
			"width": "3em",
			"position": "absolute",
			"top": "100%",
			"left": 0,
			"backgroundColor": "#222222"
		}
	},
	"repeating-linear-gradient": {
		"label": "斜めしましま下線",
		"style": {
			"position": "relative",
			"zIndex": 1
		},
		"after": {
			"content": "\"\"",
			"position": "absolute",
			"left": 0,
			"bottom": "3px",
			"width": "100%",
			"height": "5px",
			"color": "#cccccc",
			"zIndex": -1,
			"--ystdtb-custom-header-after-color-rbga-opacity": 0.3,
			"background": "repeating-linear-gradient(-45deg,var(--ystdtb-custom-header-after-color-rgba),var(--ystdtb-custom-header-after-color-rgba) 2px,#fff 0,#fff 4px)"
		}
	},
	"text-center-border": {
		"label": "左右に直線",
		"style": {
			"display": "flex",
			"gap": "0.5em"
		},
		"before": {
			"backgroundColor": "#222222",
			"height": "2px",
			"alignSelf": "center",
			"flexGrow": 1,
			"minWidth": "10%"
		},
		"after": {
			"backgroundColor": "#222222",
			"height": "2px",
			"alignSelf": "center",
			"flexGrow": 1,
			"minWidth": "10%"
		}
	},
	"balloon": {
		"label": "吹き出し",
		"style": {
			"backgroundColor": "#d4e7f2",
			"padding": {
				"top": "0.5em",
				"right": "0.7em",
				"bottom": "0.5em",
				"left": "0.7em"
			}
		},
		"after": {
			"border": {
				"top": {
					"width": "0.9em",
					"style": "solid",
					"color": "var(--ystdtb-custom-header-after-bg-color,var(--ystdtb-custom-header-bg-color))"
				},
				"right": {
					"width": "0.9em",
					"style": "solid",
					"color": "transparent"
				},
				"bottom": {
					"width": "0.9em",
					"style": "solid",
					"color": "transparent"
				},
				"left": {
					"width": "0.9em",
					"style": "solid",
					"color": "transparent"
				}
			},
			"width": 0,
			"height": 0,
			"position": "absolute",
			"top": "100%",
			"left": "1.5em"
		}
	},
	"ribbon": {
		"label": "リボン",
		"style": {
			"backgroundColor": "#eeeeee",
			"padding": {
				"top": "0.5em",
				"right": "0.5em",
				"bottom": "0.5em",
				"left": "0.5em"
			}
		},
		"after": {
			"content": "\"\"",
			"position": "absolute",
			"top": "100%",
			"left": 0,
			"border": {
				"top": {
					"width": 0,
					"style": "solid",
					"color": "transparent"
				},
				"right": {
					"width": "1em",
					"style": "solid",
					"color": "var(--ystdtb-custom-header-after-color)"
				},
				"bottom": {
					"width": "0.5em",
					"style": "solid",
					"color": "transparent"
				},
				"left": {
					"width": 0,
					"style": "solid",
					"color": "transparent"
				}
			},
			"borderRadius": 0,
			"fontSize": "20px",
			"color": "#aaaaaa"
		}
	},
	"icons": {
		"label": "リボン",
		"style": {
			"display": "flex",
			"gap": "0.5em",
			"alignItems": "center"
		},
		"before": {
			"icon": "award",
			"fontSize": "1.2em",
			"width": "1em",
			"height": "1em"
		}
	}
}
